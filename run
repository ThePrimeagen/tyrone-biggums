#!/usr/bin/env bash

docker kill $(docker ps -q)
docker run -e SERVER_PORT=42069 -e SERVER_STYLE=$1 -d --rm -it -p 42069:42069 ts-ws-test
sleep 1
node=$(pgrep node)
time=$3

if [ -z $time ]; then
                $time=$((60*4))
fi

echo "./measure $node $($((60*4))) $2"
./measure $node $time $2

docker kill $(docker ps -q)




